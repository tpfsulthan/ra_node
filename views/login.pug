  <title>SB Admin 2 - Login</title>

  <!-- Custom fonts for this template-->
  <link href="../sb/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template-->
  <link href="../sb/css/sb-admin-2.min.css" rel="stylesheet">

</head>

<body class="bg-gradient-primary">

  <div class="container">

    <!-- Outer Row -->
    <div class="row justify-content-center">

      <div class="col-xl-6 col-lg-6 col-md-6">

        <div class="card o-hidden border-0 shadow-lg my-5">
          <div class="card-body p-0">
            <!-- Nested Row within Card Body -->
            <div class="row">
              <div class="col-lg-12">
                <div class="p-5">
                  <div class="text-center">
                    <h1 class="h4 text-gray-900 mb-4">Welcome to RA</h1>
                  </div>
                  <form class="user">
                    <div class="form-group">
                      <input type="email" class="form-control form-control-user" id="username" aria-describedby="emailHelp" placeholder="Username">
                    </div>
                    <div class="form-group">
                      <input type="password" class="form-control form-control-user" id="password" placeholder="Password">
                    </div>
                    <a href="javascript:;" class="btn btn-primary btn-user btn-block loginUser">Login</a>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>

      </div>

    </div>

  </div>
  <div class="col-lg-2">
    <div id="snackbar"></div>    
  </div>

  <!-- Bootstrap core JavaScript-->
  
  <script src="../nm/jquery/dist/jquery.slim.min.js"  crossorigin="anonymous"></script>
  <script src="../nm/bootstrap/dist/js/bootstrap.bundle.min.js"  crossorigin="anonymous"></script>
  <script src="../nm/jquery/dist/jquery.min.js"  crossorigin="anonymous"></script>
  <script src="../nm/underscore/underscore-min.js"></script>
  
  <!-- Core plugin JavaScript-->
  <script src="../nm/jquery.easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="../sb/js/sb-admin-2.min.js"></script>
  <link href="../nm/toastr/build/toastr.css" rel="stylesheet"/>
  <script src="../nm/toastr/build/toastr.min.js"></script>
  <script src="../javascripts/custom.js"></script>
  

</body>

</html>